      }

      /* Mobile menu */
      #mobile-menu[aria-hidden="true"] { display: none; }
      #mobile-menu[aria-hidden="false"] { display: block; }

      /* FAQ accordion */
      .faq-item .faq-answer { display: none; }
      .faq-item.open .faq-answer { display: flex; }
      .faq-item .faq-icon-plus { display: block; }
      .faq-item.open .faq-icon-plus { display: none; }
      .faq-item .faq-icon-minus { display: none; }
      .faq-item.open .faq-icon-minus { display: block; }

      /* Testimonial hanging quotes */
      .testimonial-quote > p:first-child::before {
        content: '\201C';
        position: absolute;
        display: inline;
        transform: translateX(-100%);
      }
      .testimonial-quote > p:last-child::after {
        content: '\201D';
        display: inline;
      }
    </style>
